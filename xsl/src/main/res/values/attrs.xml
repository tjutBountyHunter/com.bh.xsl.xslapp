<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="app_primary_color" format="reference|color"/>

    <attr name="app_content_bg_color" format="reference|color"/>

    <declare-styleable name="YMULevelText">
        <attr name="leadHeight" format="dimension"></attr>
        <attr name="numHeight" format="dimension"></attr>
        <attr name="text" format="string"></attr>
        <attr name="levelText" format="integer"></attr>
        <attr name="textColor" format="color"></attr>
        <attr name="backgroundColor" format="color"></attr>
    </declare-styleable>


    <declare-styleable name="YMULevelProgresBar">
        <attr name="type" format="integer" />
        <attr name="progress_color" format="color"></attr>
        <attr name="background_color" format="color"></attr>
        <attr name="maxValue" format="integer"></attr>
        <attr name="value" format="integer"></attr>
    </declare-styleable>

</resources>